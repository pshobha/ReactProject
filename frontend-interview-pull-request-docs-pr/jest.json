{
	"coveragePathIgnorePatterns": ["<rootDir>/node_modules"],
	"collectCoverageFrom": ["src/**/*.js"],
	"transform": {
		".*": "./node_modules/babel-jest"
	},
	"testRegex": "<rootDir>/src/.*\\.test\\.js$",
	"rootDir": "./",
	"modulePaths": ["./"],
	"moduleNameMapper": {
		"^.+\\.(css|scss)$": "identity-obj-proxy"
	},
	"snapshotSerializers": [
		"<rootDir>/node_modules/enzyme-to-json/serializer"
	],
	"globals": {
		"window": true
	}
}

